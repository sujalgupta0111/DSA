package pattren_number;

import java.util.Scanner;

public class lasrgetsecondinarray {
	
	public static String num(int arr[])
	{
		String  ans="";
		int s=1;
		int f=0;
		
		for(int i=0; i<arr.length;i++)
		{
			if((arr[i]-arr[s])!=1)
			{
				ans = ans+""+arr[f]+"-"+arr[i]+",";
				f=s;
			}
			
		}
		ans = ans+""+arr[f]+"-"+arr[i]+",";
		
	 return ans;	
	}
	public static void main(String ...strings )
	{
	   
		
	}

}
